# Environment Configuration for Raspberry Pi
NODE_ENV=production
DEVICE_TYPE=pi

# Feature Flags
ENABLE_ADVANCED_FEATURES=false
ENABLE_VIDEO_DOWNLOAD=true
ENABLE_SSO=false

# Performance Settings
MAX_CACHE_SIZE=50
VIDEO_QUALITY=720p

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=prasco
DB_USER=prasco
DB_PASSWORD=prasco_secure

# Server
PORT=3000
HOST=0.0.0.0

# Logging
LOG_LEVEL=info
